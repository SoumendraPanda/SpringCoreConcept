	<bean id="countryList" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="in"></ref>
				<ref bean="us"></ref>
				<ref bean="de"></ref>
				<ref bean="jp"></ref>
			</list>
		</constructor-arg>
	</bean>

</beans>